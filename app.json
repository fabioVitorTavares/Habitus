{
  "expo": {
    "name": "Habitus",
    "slug": "Habitus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#362FD9"
    },
    "splash": {
      "image": "./assets/splashv2.png",
      "resizeMode": "contain",
      "backgroundColor": "#362FD9"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fabio.furtado.Habitus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e0d2ced7-82e4-49ce-909a-b51a264c616d"
      }
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ]
  }
}
